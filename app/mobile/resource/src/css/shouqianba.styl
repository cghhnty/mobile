@import 'nib';

.my-center {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  overflow-x: hidden;
}
@media (min-width: 1024px) {
  .my-center {
    width: 740px;
    left: 50%;
    margin-left: -370px;
  }
}

.my-center-red {
  color: #ff4e4e !important;
}

.my-center-header {
  position: relative;
  color: #fff;
  background-size: 100% 100%;
  //height: 242px;
  height: 212px;
  background-image: url(../img/my-center/banner_new@2x.png);
}

.my-center-group{
  position: relative;
}

.my-center-logo{
  width: 76px;
  height:25px;
  background-image: url(../img/common/shouqianba_logo@2x.png)
  background-size: contain;
  position: absolute;
  top: 12px;
  left: 18px;
  background-repeat: no-repeat;

}

.my-center-vendor-name{
  font-size: 15px;
  color: #160c19;
  padding-top: 12px;
  margin-left: 102px;
}

.my-center-profile{
  position: relative;
  //when adding Pin tuan, delete margin-top
  margin-top: 5%;
}
.my-center-face-set{
  display: inline-block;
  width: 49%;
  padding-left: 7%;

}

.my-center-detail-set{
  display: inline-block;
  width: 49%;
  position: absolute;
  top: 10%;
  left: 49%;

}

.my-center-member-avatar {
  float: left;
  background-color: #FFF;
  background-repeat: no-repeat;
  background-size: cover;
  -webkit-background-size: cover;
  margin: 5px 11px 24px;
  width: 98px;
  height: 98px;
  border: 4px solid #FFFFFF;
  -webkit-border-radius: 50%;
  border-radius: 50%;


}


.my-center-member-level{
  background-image: url(../img/my-center/member-level@2x.png);
  background-repeat: no-repeat;
  background-size: 120px 30px;
  background-position-y: 90px;
  position: relative;
  height: 120px;
  width: 120px;

}

.my-center-member-level-text{
  font-size: 13px;
  text-align: center;
  width: 100%;
  position: absolute;
  bottom: 8px;
}

.my-center-member-name {
  color: #313C4A;
  font-size: 16px;
  font-weight: bold;
}

.my-center-member-info {
  line-height: 1.8;
  color: #313C4A;
  font-size: 14px;
  font-weight: bold;
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='100%' height='1'><rect fill='#313C4A' x='0' y='0' width='100%' height='0.5'/></svg>");
  background-position: 0 100%;
  background-repeat: no-repeat;
  margin-bottom: 5px;
}
.my-center-member-devider{
  width: 100%;
  margin-top: 10px;
  margin-bottom: 10px;
}

.my-center-icon-inline{
  position: absolute;
  background-repeat: no-repeat;
  background-size: contain;
  height: 15px;
  width: 30px;
  margin-top: 3px;
}

.my-center-text-inline{

  padding-left: 18px;
  height: 25px;
}

.my-center-icon-redmoney{
  background-image: url(../img/my-center/redmoney@2x.png);
}

.my-center-icon-score{
  background-image: url(../img/my-center/score@2x.png);
}

.my-center-member-hbjf {
  position: absolute;
  width: 100%;
  bottom: 0;
  padding-bottom: 4px;
}

.my-center-member-hbjf-item {
  color: #313C4A;
  font-size: 14px;
}

.my-center-member-hbjf-splitter {
  float: left;
  margin-left: -1px;
  width: 1px;
  height: 13px;
  background-color: #fff;
  margin-top: 4px;
}
.my-center-member-groupon{
  display block;
  background-color: rgba(255,255,255,0.6);
  color: black;
  border-radius: 2px;
  width: 80%;
  margin-top: 10px;
  margin-left: 10%;
  height: 34px;
  border: 1px;
  padding-top: 5px;
  position: relative;
}

.my-center-member-groupon-text{
  padding-top: 2px;
  padding-left: 40px;
}

.my-center-member-groupon-text-right{
  position: absolute;
  right: 8%;
  color: #4489dd;
  font-size: 14px;
  top: 7px;
}

.my-center-member-groupon-arrow{
  position: absolute;
  height: 14px;
  right: 3%;
  top: 11px;
}

.my-center-cart-icon{
  background-image: url(../img/my-center/cart@2x.png);
  position: absolute;
  background-repeat: no-repeat;
  background-size: contain;
  height: 22px;
  width: 30px;
  margin-left: 5px;

}
.my-center-content {
  overflow: hidden;
  padding: 2px;
  padding-left: 4px;
  padding-right: 4px;
}

.my-center-item-wrap {
  position: relative;
  float: left;
  width: 50%;
  overflow: hidden;
  height: 96px;
}

.my-center-item-wrap:before {
  content: "";
  display: block;
  padding-top: 100%;
}

.my-center-item {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  display: block;
  text-decoration: none;
  border: 1px solid #dadada;
  border-radius: 4px;
  background-color: #fff;
  margin: 2px;
}

.my-center-item-content {
  position: absolute;
  width: 100%;
  top: 50%;
  transform: translateY(-50%);
  height: 70px;
}

.my-center-item-dot {
  position: relative;
  top: -5px;
  margin-left: 3px;
  display: inline-block;
  width: 6px;
  height: 6px;
  border-radius: 100px;
  background-color: #ff4e4e;
}

.my-center-item-dot2 {
  position: absolute;
  top: -1px;
  right: -10px;
  width: 6px;
  height: 6px;
  border-radius: 100px;
  background-color: #ff4e4e;
}

.my-center-item:hover {
  text-decoration: none;
}

.my-center-item-icon {
  margin-top: 8px;
  height: 33px;
  margin-bottom: 0;
  text-align: center;
}
.my-center-item-icon-second {
  margin: 8px;
  height: 33px;
  margin-bottom: 0;
  margin-left: 40%;
}
.my-center-item-text {
  display: inline-block;
  vertical-align: middle;
  color: rgb(68,137,221);
  padding-top: 5px;
  font-size: 15px;
}

.my-center-item-text-second {
  color: rgb(68,137,221);
  font-size: 15px;
  text-align: center;
}

.my-center-item-comment {
  //color: #adadad;
  color: rgb(49,60,74);
  font-size: 14px;
  text-align: center;

}

.my-center-value {
  float: right;
  color: #c7c7cc;
  white-space: nowrap;
}

.my-center-icon {
  height: 23px;
}

.my-center-icon-block{
  height: 24px;
  position: relative;
  display: inline;
}
.my-center-icon-second {
  width: 20%;
  height: 23px;
  padding-left: 40%;
}

.my-center-icon-balance {
  background-image: url(../img/my-center/balance@2x.png);
}

.my-center-icon-card {
  background-image: url(../img/my-center/card@2x.png);
}

.my-center-icon-clock {
  background-position: -92px -35px
}

.my-center-icon-home {
  background-image: url(../img/my-center/branch@2x.png);
}

.my-center-icon-order {
  background-image: url(../img/my-center/record@2x.png);
}

.my-center-icon-user {
  background-image: url(../img/my-center/capture@2x.png);
}

.my-center-icon-voucher {
  background-image: url(../img/my-center/voucher@2x.png);
}

@font-face {
  font-family: "方正兰亭特黑_GBK";

  src: url("data:application/x-font-ttf;charset=utf-8;base64,") format("truetype");

  font-style: normal;
  font-weight: normal;
}

.my-center-promotions-container {
  clear: both;
  height: 165px;
  margin-top: 12px;

  .activity-banner-cutoff-symbol {
    position: absolute;
    width: 87px;
    height: 110px;
    left: round(28px/2);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    z-index: 1000;

    .min-participants {
      position: absolute;
      width: 66px;
      height: 30px;
      top: 10px;
      left: -2px;
      color: #0A4F9C;
      text-align: center;

      .min-participants-number {
        font-family: "方正兰亭特黑_GBK";
        font-size: 1.8rem;
      }
      .min-participants-label {
        font-family: "方正兰亭特黑_GBK";
        font-size: 1.4rem;
        margin-left: 2px;
      }
    }
    .activity-cutoff {
      position: absolute;
      width: 40px;
      height: 40px;
      top: 40px;
      left: 10px;
      text-align: center;

      .activity-cutoff-number {
        font-family: "方正兰亭特黑_GBK";
        font-size: 2.9rem;
        color: #ffffff;
      }
    }
  }
  .activity-banner-change-gradually-layout {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 40%;
    background: -moz-linear-gradient(top, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1));
    background: -webkit-gradient(linear, 0 0, 0 100%, from(rgba(255, 255, 255, 0)), to(rgba(255, 255, 255, 1)));
    z-index: 1000;
  }
  .activity-item {
    position: relative;
    width: 100%;
    height: round(330px/2);
    margin-bottom: round(24px/2);

    .activity-banner{
      position: relative;
      display: block;
      width: 100%;
      height: 100%;
      overflow: hidden;

      img {
        width: 100%;
      }

    }
    .activity-banner-cutoff-symbol {
      top: -8px;
      left: 8px;
      background-image: url(../img/my-center/cutOff@2x.png);
    }
    .banner-count-intro {
      position: absolute;
      bottom: 0;
      right: 8px;
      text-align: right;
      z-index: 1000;
      .banner-intro-up {
        height: round(60px/2);
        line-height: round(60px/2);
        font-size: (3.2rem/2);
        color: rgb(255, 255, 255);
        p {
          float: right;
          height: 30px;
        }
        .intro-head {
          img {
            width: 23px;
            height: 31px;
            vertical-align: baseline;
          }
        }
        .intro-list {
          background: url(../img/my-center/nameCenter@2x.png) repeat-x;
          background-size: auto 31px;
        }
        .intro-end {
          img {
            width: 16px;
            height: 31px;
            vertical-align: baseline;
          }
        }
      }
      .banner-intro-down {
        margin-right: 5px;
        font-size: (2.8rem/2);
        color: rgb(1,1,0);
        clear: both;

        .origin-price, .promotion-price-label {
          display: inline-block;
          vertical-align: top;
          padding-top: 9px;
        }
        .promotion-price-label {
          margin-left: 15px;
        }

        .promotion-price-number {
          font-size: (4.8rem/2);
          color: rgb(255, 38, 155);
          text-shadow: 1px 1px 1px rgb(156, 57, 23);
          padding-left: 5px;
        }
      }
    }
  }
}

.islider-dom {
  overflow: visible !important;
}

.islider-dot-wrap {
  position: relative !important;
  margin: 5px auto !important;
}

.my-center-bottom {
  position: absolute;
  height: 31px;
  width: 100%;
  padding-bottom: 10px;

  bottom: 0;
  right: 0;
  display: table;
  box-sizing: border-box;
  border-top: 1px solid $border-color;
  background-color: #eee;

  color: #000;
  .logo {
    width: 100%;
    height: 100%;
    background-image: url(../img/my-center/logo.png) ;
    background-position: center center  ;
    background-size: auto 80%;
    -webkit-background-size: auto 80%;
    background-repeat: no-repeat
  }
}

#myCenterRenovating {
  position: fixed;
  display: none;
  left: 50%;
  top: 50%;
  margin: -25px 0 0 -85px;
  width: 170px;
  height: 50px;
  background-color: rgba(0,0,0,.5);
  border-radius: 4px;
  z-index: 10000;
  text-align: center;
  line-height: 50px;
  color: #ffffff;
}
