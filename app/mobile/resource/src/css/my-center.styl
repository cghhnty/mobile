@font-face {
  font-family: "方正兰亭特黑_GBK";

  src: url("data:application/x-font-ttf;charset=utf-8;base64,") format("truetype") /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */;

  font-style: normal;
  font-weight: normal;
}

.my-center-promotions-container {
  position: relative;
  clear: both;
  margin: 12px 0 25px 0;

  .my-center-promotions {
    height: 165px;
  }

  .activity-banner-cutoff-symbol {
    position: absolute;
    width: 87px;
    height: 110px;
    left: round(28px/2);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    z-index: 1000;

    .min-participants {
      position: absolute;
      width: 66px;
      height: 30px;
      top: 12px;
      left: -2px;
      color: #0A4F9C;
      text-align: center;

      .min-participants-number {
        font-family: "方正兰亭特黑_GBK";
        font-size: 1.8rem;
      }
      .min-participants-label {
        font-family: "方正兰亭特黑_GBK";
        font-size: 1.4rem;
        margin-left: 2px;
      }
    }
    .activity-cutoff {
      position: absolute;
      width: 75px;
      height: 40px;
      top: 40px;
      left: 5px;
      text-align: center;

      .activity-cutoff-number {
        font-family: "方正兰亭特黑_GBK";
        font-size: 2.6rem;
        color: #ffffff;
      }
      .activity-cutoff-label {
        font-family: "方正兰亭特黑_GBK";
        font-size: 2rem;
        color: #ffffff;
      }
    }
  }
  .activity-banner-change-gradually-layout {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 40%;
    background: -moz-linear-gradient(top, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1));
    background: -webkit-gradient(linear, 0 0, 0 100%, from(rgba(255, 255, 255, 0)), to(rgba(255, 255, 255, 1)));
    z-index: 1000;
  }
  .activity-item {
    position: relative;
    width: 100%;
    height: round(330px/2);
    margin-bottom: round(24px/2);

    .activity-banner{
      position: relative;
      display: block;
      width: 100%;
      height: 100%;
      overflow: hidden;

      img {
        width: 100%;
      }

    }
    .activity-banner-cutoff-symbol {
      top: -9px;
      left: 8px;
      background-image: url(../img/my-center/cutOff@2x.png);
    }
    .banner-count-intro {
      position: absolute;
      bottom: 14px;
      right: 8px;
      text-align: right;
      line-height: 8px;
      z-index: 1000;
      .banner-intro-up {
        height: round(60px/2);
        line-height: round(60px/2);
        font-size: (3.2rem/2);
        color: rgb(255, 255, 255);
        p {
          float: right;
          height: 30px;
        }
        .intro-head {
          img {
            width: 23px;
            height: 31px;
            vertical-align: baseline;
          }
        }
        .intro-list {
          background: url(../img/my-center/nameCenter@2x.png) repeat-x;
          background-size: auto 31px;
        }
        .intro-end {
          img {
            width: 16px;
            height: 31px;
            vertical-align: baseline;
          }
        }
      }
      .banner-intro-down {
        margin-right: 5px;
        font-size: (2.8rem/2);
        color: rgb(1,1,0);
        clear: both;

        .origin-price, .promotion-price-label {
          display: inline-block;
          vertical-align: top;
          padding-top: 2px;
        }
        .promotion-price-label {
          margin-left: 15px;
        }

        .promotion-price-number {
          font-size: (4.8rem/2);
          color: rgb(255, 38, 155);
          text-shadow: 1px 1px 1px rgb(156, 57, 23);
          padding-left: 5px;
        }
      }
    }
  }


    .islider-dom {
      overflow: visible;
    }
  .islider-dot-wrap {
    bottom: -15px;
    z-index: 1000;

    .islider-dot {
      border: none;
      width: 6px;
      height: 6px;
      background-color: #9a9a9a;

      &.active {
        background-color: black;
      }
    }
  }

}

.my-center-bottom {
	height: 31px;
	width: 100%;
  padding-bottom: 10px;
	
	bottom: 0;
    right: 0;
    display: table;
    box-sizing: border-box;
    border-top: 1px solid $border-color;
  background-color: #eee;
    
    color: #000;
  .logo {
  	width: 100%;
  	height: 100%;		
  	background-image: url(../img/my-center/logo.png) ;
    background-position: center center  ;
    background-size: auto 80%;
    -webkit-background-size: 72px 17px;
    background-repeat: no-repeat
  }
}
