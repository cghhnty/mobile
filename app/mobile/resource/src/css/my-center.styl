.my-center-content {
	overflow: hidden;
	padding: 2px;
	padding-left: 4px;
    padding-right: 4px;
}

.my-center-item-wrap {
	position: relative;
	float: left;
	width: 33.333%;
	overflow: hidden;
	height: 74px;
}

.my-center-item-wrap:before {
	content: "";
	display: block;
	padding-top: 100%;
}

.my-center-item {
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	display: block;
	text-decoration: none;
	border: 1px solid #dadada;
	border-radius: 4px;
	background-color: #fff;
	margin: 2px;
}

.my-center-item-content {
	position: absolute;
	width: 100%;
	top: 50%;
	transform: translateY(-50%);
	height: 70px;
}

.my-center-item-dot {
	position: relative;
	top: -5px;
	margin-left: 3px;
	display: inline-block;
	width: 6px;
	height: 6px;
	border-radius: 100px;
	background-color: #ff4e4e;
}

